{
    "manifest_version": 3,
    "name": "Nebula",
    "version": "1.1",

    "description": "A comfy homepage for your browser",
    


      "chrome_url_overrides": {
        "newtab": "index.html",
        "homepage": "index.html"
    },
    


    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["script.js"]
          
        }
      ],

      "permissions": [
        "search" 
      ]
      
    
}
